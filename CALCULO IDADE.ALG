algoritmo "calculo idade"
var
 ANO_ATUAL, ANO_NASCIMENTO, RESULTADO: Inteiro
inicio
   Escreva("Informe o ano atual ")
   Leia (ANO_ATUAL)
   Escreva("Informe o ano do seu nascimento ")
   Leia (ANO_NASCIMENTO)
   RESULTADO <- ANO_ATUAL - ANO_NASCIMENTO
   Escreva ("Você tem ", RESULTADO, " anos!" )

fimalgoritmo